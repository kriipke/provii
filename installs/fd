github sharkdp/fd fd-.*x86_64.*unknown-linux-musl\.tar\.gz$
tar -xf * && cd fd*/

install fd

install fd.1 $PREFIX/share/man/man1/ \
	|| warn "fd installed but failed to install man page" 
install autocompletion/fd.bash-completion $CFG/bash-completion.d/ \
	|| warn "fd installed but failed to install bash completions" 

